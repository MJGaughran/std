# sample database using all the different types of statistics
record(ai,"$(P)memory")
{
	field(DTYP,"VX stats")
	field(SCAN,"I/O Intr")
	field(INP,"@memory")
	field(HOPR,"100.0")
	field(HIHI,"90.0")
	field(HIGH,"60.0")
	field(HHSV, "MAJOR")
	field(HSV, "MINOR")
}
record(ai,"$(P)cpu")
{
	field(DTYP,"VX stats")
	field(SCAN,"I/O Intr")
	field(INP,"@cpu")
	field(HOPR,"100.0")
	field(HIHI,"90.0")
	field(HIGH,"60.0")
	field(HHSV, "MAJOR")
	field(HSV, "MINOR")
}
record(ai,"$(P)fd")
{
	field(DTYP,"VX stats")
	field(SCAN,"I/O Intr")
	field(INP,"@fd")
	field(HOPR,"100.0")
	field(HIHI,"90.0")
	field(HIGH,"60.0")
	field(HHSV, "MAJOR")
	field(HSV, "MINOR")
}
record(ao,"$(P)memoryScanPeriod")
{
	field(DTYP,"VX stats")
	field(OUT,"@memoryScanPeriod")
}
record(ao,"$(P)fdScanPeriod")
{
	field(DTYP,"VX stats")
	field(OUT,"@fdScanPeriod")
}
record(ao,"$(P)cpuScanPeriod")
{
	field(DTYP,"VX stats")
	field(OUT,"@cpuScanPeriod")
}

# task watch-dog - see iocCheck.c
record(sub,"$(P)status") {
    field(DESC,"$(P) task status")
    field(ASG,"")
    field(SCAN,"10 second")
    field(PINI,"YES")
    field(PHAS,"0")
    field(EVNT,"0")
    field(TSEL,"0")
    field(DISV,"1")
    field(SDIS,"0")
    field(DISS,"NO_ALARM")
    field(PRIO,"LOW")
    field(FLNK,"0")
    field(INAM,"iocCheckInit")
    field(SNAM,"iocCheck")
    field(INPA,"0")
    field(INPB,"0")
    field(INPC,"0")
    field(INPD,"0")
    field(INPE,"0")
    field(INPF,"0")
    field(INPG,"0")
    field(INPH,"0")
    field(INPI,"0")
    field(INPJ,"0")
    field(INPK,"0")
    field(INPL,"0")
    field(EGU,"")
    field(HOPR,"0")
    field(LOPR,"0")
    field(HIHI,"10")
    field(LOLO,"0.3")
    field(HIGH,"9")
    field(LOW,"0.4")
    field(PREC,"2")
    field(BRSV,"NO_ALARM")
    field(HHSV,"NO_ALARM")
    field(LLSV,"NO_ALARM")
    field(HSV,"NO_ALARM")
    field(LSV,"MAJOR")
    field(HYST,"0")
    field(ADEL,"0")
    field(MDEL,"0")
}

